<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eventappprod">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Register" />
        <activity
            android:name=".ArchivedEventsActivity"
            android:label="Return to Profile"
            android:parentActivityName=".Profile" />
        <activity
            android:name=".PreviousEventsActivity"
            android:label="Return to Profile"
            android:parentActivityName=".Profile" />
        <activity
            android:name=".FriendsListActivity"
            android:label="Return to Profile"
            android:parentActivityName=".Profile" />
        <activity
            android:name=".EventActivity"
            android:label="Return to Dashboard"
            android:parentActivityName=".DashBoard" />
        <activity android:name=".Search" />
        <activity android:name=".DashBoard" />
        <activity android:name=".Profile"/>
        <activity
            android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>